{
  "name": "phylotree-visualization-demo",
  "version": "1.2.5",
  "private": false,
  "description": "A library used for visualizing phylotree.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "file": [
    "/dist"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run clean && tsc && npm run copy-files",
    "clean": "rimraf dist",
    "copy-files": "copyfiles -u 3 -E **/styles/**.png dist/components/styles && copyfiles -u 3 -E **/styles/**.css dist/components/styles",
    "prepare": "npm run build"
  },
  "author": "nktd",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.1",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "@types/d3-array": "^3.0.3",
    "@types/d3-scale": "^4.0.2",
    "@types/d3-scale-chromatic": "^3.0.0",
    "@types/d3-shape": "^3.1.0",
    "@types/file-saver": "^2.0.5",
    "bootstrap": "^5.2.3",
    "d3-array": "^3.2.0",
    "d3-react-axis": "^0.0.3",
    "d3-scale": "^4.0.2",
    "d3-scale-chromatic": "^3.0.0",
    "d3-shape": "^3.1.0",
    "file-saver": "^2.0.5",
    "phylotree": "^1.0.16",
    "react-zoom-pan-pinch": "^2.1.3",
    "save-svg-as-png": "^1.4.17"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
